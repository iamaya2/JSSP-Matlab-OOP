function [] = InstanceLoader () 
delimiterIn = ' ' 
headerlinesIn = 3
nbjobs=15;
nbmachines=15;
mP1 = importdata("tai15_15.txt",delimiterIn,headerlinesIn)
mM1 = importdata("tai15_15.txt",delimiterIn,headerlinesIn+16)
disp(mM1.data)
for idx=1:10 
    mP(idx)= importdata("tai15_15.txt",delimiterIn,headerlinesIn)
    mM(idx)= importdata("tai15_15.txt",delimiterIn,headerlinesIn+nbjobs+1)
    headerlinesIn = (idx+1)*headerlinesIn+ (nbjobs + nbmachines + 1)*idx
end